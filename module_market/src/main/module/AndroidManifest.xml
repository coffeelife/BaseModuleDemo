<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wss.module.market">


    <!--在Module单独开发模式下使用该mainfest-->
    <application
        android:name=".common.MarketApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AdmTheme">

        <activity android:name=".ui.main.MarketMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.goods.detail.GoodsDetailActivity" />
        <activity android:name=".ui.goods.cart.ShoppingCartActivity" />
    </application>
</manifest>